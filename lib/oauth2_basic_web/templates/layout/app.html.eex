          </a>

          <a role="button" class="navbar-burger" aria-label="menu" aria-expanded="false">
            <span aria-hidden="true"></span>
            <span aria-hidden="true"></span>
            <span aria-hidden="true"></span>
          </a>
        </div>
      </nav>
    </header>
    <main role="main" class="container">
      <p class="alert alert-info my-4" role="alert"><%= get_flash(@conn, :info) %></p>
      <p class="alert alert-danger my-4" role="alert"><%= get_flash(@conn, :error) %></p>
      <%= @inner_content %>
    </main>
